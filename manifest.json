{
"manifest_version": 2,
"name": "My Cool Extension",
"version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search?q=miuta+ioan+adrian&oq=miuta*"
      ],
      "js": ["jquery-3.4.1.min.js", "content.js"]
    }
